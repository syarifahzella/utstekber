<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tekber">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".materi_content"></activity>
        <activity android:name=".materi_broadcast" />
        <activity android:name=".materi_service" />
        <activity android:name=".materi_intent" />
        <activity android:name=".materi_activity" />
        <activity android:name=".content" />
        <activity android:name=".broadcast" />
        <activity android:name=".service" />
        <activity android:name=".intent" />
        <activity android:name=".activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>